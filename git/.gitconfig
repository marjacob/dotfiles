[user]
	email = marjacob@users.noreply.github.com
	name = marjacob
[init]
	defaultBranch = master
[pull]
	rebase = true
[rebase]
	autostash = true
[submodule]
	recurse = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	process = git-lfs filter-process
	smudge = git-lfs smudge -- %f
	required = true
[alias]
	uncommit = reset --soft HEAD^
	unstage = reset HEAD --
[include]
	path = ~/.gitconfig.local
